{
  "name": "student-faculty-management-system",
  "version": "1.0.0",
  "description": "A comprehensive student and faculty management system",
  "main": "server/index.js",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "mix:development": "mix",
    "mix:watch": "mix watch",
    "mix:watch-poll": "mix watch -- --watch-options-poll=1000",
    "mix:hot": "mix watch --hot",
    "mix:prod": "mix --production",
    "mix:clean": "rm -rf public/js public/css public/mix-manifest.json",
    "lint": "eslint resources/js/**/*.{js,jsx}",
    "format": "prettier --write 'resources/js/**/*.{js,jsx}'"
  },
  "keywords": [
    "student",
    "faculty",
    "management",
    "education"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-slot": "^1.0.2",
    "apexcharts": "^5.3.6",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.5.1",
    "clsx": "^2.0.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.553.0",
    "react": "^18.2.0",
    "react-apexcharts": "^1.8.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.1",
    "recharts": "^2.15.4",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "@babel/preset-react": "^7.23.3",
    "autoprefixer": "^10.4.16",
    "laravel-mix": "^6.0.49",
    "postcss": "^8.4.31",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.69.5",
    "sass-loader": "^12.6.0",
    "tailwindcss": "^3.3.5"
  }
}
